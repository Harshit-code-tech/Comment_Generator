{
  "version": 2,
  "builds": [
    {
      "src": "run.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "run.py"
    }
  ],
  "functions": {
    "api/*": {
      "memory": 1024,
      "maxDuration": 60
    }
  }
}
